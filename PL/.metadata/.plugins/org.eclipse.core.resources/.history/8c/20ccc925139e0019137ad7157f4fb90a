#include <stdio.h>
#include <stdlib.h>

int main(void)
{
	int c,m=0,n=0;
	int set1[m],set2[n];

	input(int set1[],int m);
	input(int set2[],int n);

	display();

    printf("Enter Your Choice"
    		"1.Input\n"
    		"2.Output\n"
    		"3.Union\n"
			"4.Intersection\n"
    		"5.Difference\n"
    		"6.Symmetric Difference\n"
    		"7.Exit\n");

    scanf("%d",&c);

    switch(c)
    {
       case 1: input();

       case 2: display();

       case 3: printf("Union");


       case 4: printf("Intersection");

       case 5: printf("Difference");

       case 6: printf("Symmetric Difference");

       case 7: printf("Exit");
               break;


    default: printf("Enter Correct Choice");
             break;
    }


	return EXIT_SUCCESS;
}

