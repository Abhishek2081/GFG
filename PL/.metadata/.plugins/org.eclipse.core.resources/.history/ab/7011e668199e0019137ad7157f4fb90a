#include <stdio.h>
#include <stdlib.h>

int main(void)
{
	int c,m=0,n=0;
	int set1[10],set2[10];

    printf("Enter Your Choice"
    		"1.Input\n"
    		"2.Output\n"
    		"3.Union\n"
			"4.Intersection\n"
    		"5.Difference\n"
    		"6.Symmetric Difference\n"
    		"7.Exit\n");

    scanf("%d",&c);

    switch(c)
    {
       case 1: input(set1,m);
               input(set2,n);

       case 2: display(set1,m);
               display(set2,n);

       case 3: printf("Union");

       case 4: printf("Intersection");

       case 5: printf("Difference");

       case 6: printf("Symmetric Difference");

       case 7: break;

       default: printf("Enter Correct Choice");
                break;
    }


	return EXIT_SUCCESS;
}

void input(int set1[],int m)
{
   int i;

   printf("Enter Size of Set\n");
   scanf("%d",&m);


   printf("Enter Elements Of Set\n");
    for(i=0;i<m;i++)
     {
        scanf("%d",&set1[i]);
     }

 }

void display(int set1[],int m)
{
	int i;

	printf("Set is\n");
	for(i=0;i<m;i++)
	{
		printf("%d",&set1[i]);
	}

 }

void Union(int set1[],int set2[])
{
    int i,m;
    for(i=0;i<m;i++)
    {
       if(set1[i]==set2[i])
       {
    	   printf("%d",set1[i]);
       }

       if(set1[i]!=set2[i])
       {
    	   printf("%d %d",set1[i],set2[i]);
       }
    }
	}
