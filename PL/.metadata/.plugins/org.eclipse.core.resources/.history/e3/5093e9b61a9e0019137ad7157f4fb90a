#include <stdio.h>
#include <stdlib.h>

int main(void)
{
	int c,m=0,n=0;
	int set1[10],set2[10];

    do
    {
	printf("Enter Your Choice"
    		"1.Input\n"
    		"2.Output\n"
    		"3.Union\n"
			"4.Intersection\n"
    		"5.Difference\n"
    		"6.Symmetric Difference\n"
    		"7.Exit\n");

    scanf("%d",&c);

    switch(c)
    {
       case 1:
    	   printf("Enter Size of Set\n");
    	       	   	   scanf("%d",&m);
    	       	       input(set1,m);
    	      printf("Enter Size of Set\n");
    	      scanf("%d",&n);

               input(set2,n);
               break;

       case 2: display(set1,m);
               display(set2,n);
               break;

       case 3: Union(set1,set2);
               break;

       case 4: printf("Intersection");

       case 5: printf("Difference");

       case 6: printf("Symmetric Difference");

       case 7: break;

       default: printf("Enter Correct Choice");
                break;
    }
    printf("Do u wanna continue?(1/0)");
    scanf("%d",&c);
    }while(c==1);


	return EXIT_SUCCESS;
}

void input(int set1[],int m)
{
   int i;


   printf("Enter Elements Of Set\n");
    for(i=0;i<m;i++)
     {
        scanf("%d",&set1[i]);
     }

 }

void display(int set1[],int m)
{
	int i;

	printf("Set is\n");
	for(i=0;i<m;i++)
	{
		printf("%d",&set1[i]);
	}

 }

void Union(int set1[],int set2[])
{
    int i,j,m,n,flag;
    for(i=0;i<m;i++)
    {
       for(j=0;j<n;j++)
       {
         if(set1[i]==set2[i])
         {
    	    printf("%d",set1[i]);
         }
       }
    }
    for(i=0;i<n;i++)
    {  flag=0;
       for(j=0;j<m;j++)
       {
         if(set1[j]==set2[i])
        	 {
        	 flag=1;
        	 break;
    	     }
       }
       if(flag==0)
   	    printf("%d",set2[i]);

	}
}


